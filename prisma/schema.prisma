// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

model Cliente {
  id    Int    @id @default(autoincrement())
  nome  String
  sobreNome String
  nascimento String
  cpf String @unique
  telefone String @unique
  email String @unique

  @@map("clientes")
}
